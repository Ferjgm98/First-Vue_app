{
  "name": "robofiends",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "tailwind:css": "./node_modules/.bin/tailwind build ./src/assets/css/tailwind.css -c ./tailwind.js -o ./public/css/styles.css",
    "serve": "npm run tailwind:css && vue-cli-service serve",
    "build": "npm run tailwind:css && vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.6.6",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "install": "^0.12.2",
    "npm": "^6.9.0",
    "tailwindcss": "^0.7.4",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
